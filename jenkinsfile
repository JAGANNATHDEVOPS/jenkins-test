pipeline {
    agent any

    stages {
        stage('RUN KUBECTL COMMAND') {
            steps {
                    withKubeConfig(caCertificate: '', clusterName: '', contextName: '', credentialsId: 'kubernetes-credentials', namespace: '', restrictKubeConfigAccess: false, serverUrl: '')
                    {
                        sh 'kubectl get ns'
                    }
            }
        }
    }
}
